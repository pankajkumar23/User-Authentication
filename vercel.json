{
    "version": 2,
    "builds": [
      {
        "src": "backend/Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/Server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }
  